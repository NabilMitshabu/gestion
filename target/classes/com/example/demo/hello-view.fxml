<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TextArea?>

<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml" fx:controller="com.example.demo.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <Button text="Submit" onAction="#onSubmitButtonClick"/>
    <Label fx:id="infoLabel"/>
    <TextArea fx:id="outputTextArea" prefWidth="600" prefHeight="400" wrapText="true" editable="false"/>
    <Button text="Exécuter" onAction="#executeCode"/>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <!-- Autres éléments de ton interface -->

</VBox>

